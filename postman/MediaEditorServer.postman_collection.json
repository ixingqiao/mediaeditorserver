		{
			"name": "提交拼接任务",
			"event": [
				{
					"listen": "prerequest",
					"script": {
						"exec": [
							"// 获取当前时间并格式化为指定格式\r",
							"const date = new Date();\r",
							"date.setHours(date.getHours() + 8); // 增加8个小时以转换为北京时间\r",
							"pm.environment.set(\"current_time\", date.toISOString().replace(\"T\", \" \").slice(0, -1));"
						],
						"type": "text/javascript"
					}
				}
			],
			"request": {
				"method": "POST",
				"header": [],
				"body": {
					"mode": "raw",
					"raw": "{\r\n    \"videoFiles\": [\"/data/huaxue/huaxue_0000.mp4\",\"/data/huaxue/huaxue_0007.mp4\", \"/data/huaxue/huaxue_0008.mp4\",\"/data/huaxue/huaxue_0021.mp4\", \"/data/huaxue/huaxue_0025.mp4\"],\r\n    \"transitions\": [\"circlecrop\", \"rectcrop\",\"slideright\",\"pixelize\"],\r\n    \"interval\": 1,\r\n    \"bgMusic\":\"/data/bgm/只要平凡.ogg\"\r\n}",
					"options": {
						"raw": {
							"language": "json"
						}
					}
				},
				"url": {
